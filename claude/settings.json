{
  "permissions": {
    "allow": [
      "Bash(./manage.py:*)",
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(curl:*)",
      "Bash(direnv:*)",
      "Bash(django-admin:*)",
      "Bash(docker:*)",
      "Bash(done)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(find:*)",
      "Bash(git:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(python -c:*)",
      "Bash(python -m py_compile:*)",
      "Bash(python manage.py:*)",
      "Bash(python3:*)",
      "Bash(uv:*)",
      "Bash(wc:*)",
      "Skill(superpowers:brainstorming)",
      "Skill(superpowers:executing-plans)",
      "Skill(superpowers:requesting-code-review)",
      "Skill(superpowers:systematic-debugging)",
      "WebSearch",
      "WebFetch"
    ],
    "defaultMode": "default"
  },
  "statusLine": {
    "type": "command",
    "command": "bash -c '\"${HOME}/.bun/bin/bun\" \"$(ls -td ~/.claude/plugins/cache/claude-hud/claude-hud/*/ 2>/dev/null | head -1)src/index.ts\"'"
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": true,
    "context7@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "python-development@claude-code-workflows": true,
    "javascript-typescript@claude-code-workflows": true,
    "backend-development@claude-code-workflows": true,
    "security-scanning@claude-code-workflows": true,
    "full-stack-orchestration@claude-code-workflows": true,
    "atlassian@claude-plugins-official": true,
    "claude-hud@claude-hud": true
  },
  "alwaysThinkingEnabled": true,
  "mcpServers": {
    "better-icons": {
      "command": "npx",
      "args": [
        "-y",
        "better-icons"
      ]
    }
  }
}
